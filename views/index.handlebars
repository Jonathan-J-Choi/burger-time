<h1>Yum yum burger time!</h1>

<h3>Eat your fill!</h3>

<ul>
  {{#each burgers}}
  	<li>
       <p>Burger: {{this.burger_name}}</p>
  	</li>
  {{/each}}
</ul>

<div>
  <form action="/" method="POST">
  	<textarea type="text" name="Burgers" id="burgerName"></textarea>

  	<button type="submit">Submit</button>
  </form>
</div>